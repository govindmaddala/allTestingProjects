<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="TestNG">
  <test name="groups related like smoke or regression">
	<groups>
		<run>
			<exclude name="reg"/>
			<include name="smoke"/>
		</run>
	</groups>
    <classes>
      <class name="_3"/>
	  <class name="_4"/>
	</classes>
  </test>
</suite> <!-- Suite -->
